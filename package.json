{
  "name": "projman",
  "version": "1.0.0",
  "description": "Project Management System",
  "main": "backend/index.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-modules backend/index.js",
    "client": "cd frontend && npm start",
    "server": "nodemon --experimental-modules backend/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd frontend && npm run build"
  },
  "keywords": [
    "project",
    "management",
    "React",
    "Node",
    "Express",
    "MongoDB"
  ],
  "author": "Raj Gohel",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "concurrently": "^7.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongodb": "^5.7.0",
    "mongoose": "^7.4.0",
    "nodemon": "^2.0.13"
  }
}
